; Manufacturers
;-------------------------------------------------
[Manufacturer]
%Philips%=Philips


; Manufacturer sections
;-------------------------------------------------
[Philips]
%082E%=082E.Install, Monitor\PHL082E

; Install sections
;-------------------------------------------------
[082E.Install]
DelReg=DEL_CURRENT_REG
AddReg=082E.AddReg,1280,DPMS
CopyFiles=082E.CopyFiles

; Common AddReg/DelReg sections
;-------------------------------------------------
[DEL_CURRENT_REG]
HKR,MODES
HKR,,MaxResolution
HKR,,DPMS
HKR,,ICMProfile

; Pre-defined AddReg sections
[1280]
HKR,,MaxResolution,,"1280,1024"

[DPMS]
HKR,,DPMS,,1


;Model AddReg sections
;-------------------------------------------------
[082E.AddReg]
HKR,"MODES\1280,1024",Mode1,,"30.0-83.0,56.0-76.0,+,+"
HKR,,ICMProfile,0,"PhLCD17a.icm"

;-------------------------------------------------------------------
[082E.CopyFiles]
