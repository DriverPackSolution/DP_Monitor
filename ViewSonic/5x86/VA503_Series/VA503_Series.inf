HKR,MODES
HKR,,MaxResolution
HKR,,DPMS 
HKR,,ICMProfile

[1024]
HKR,,MaxResolution,,"1024,768"

[DPMS]
HKR,,DPMS,,1

[VA503.AddReg]  
HKR,"MODES\1024,768",Mode1,,"30-60,50-75,-,-" 
HKR,,ICMProfile,0,"VA503_Series.ICM"

[VA503.CopyFiles]                                       
VA503_Series.ICM

[Strings]
