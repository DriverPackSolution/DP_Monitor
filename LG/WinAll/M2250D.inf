[Version]
signature="$CHICAGO$"
Class=Monitor
ClassGuid={4D36E96E-E325-11CE-BFC1-08002BE10318}
Provider=%LG%
CatalogFile=M2250D.cat
DriverVer=10/17/2010,1.0

[ControlFlags]
ExcludeFromSelect.NT=Monitor\GSM57EF

[DestinationDirs]
DefaultDestDir = 11
M2250D_Analog.CopyFiles = 23

[SourceDisksNames]
1=%DiskName%,,,

[SourceDisksFiles]
