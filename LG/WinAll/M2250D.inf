[DEL_CURRENT_REG]
HKR,MODES
HKR,,MaxResolution
HKR,,DPMS
HKR,,ICMProfile

[1920]
HKR,,MaxResolution,,"1920,1080"

[DPMS]
HKR,,DPMS,,1

[M2250D_Analog.AddReg]
HKR,"MODES\1920,1080",Mode1,,"30.0-83.0,56.0-75.0,+,+"
HKR,,PreferredMode,,"1920,1080,60"
HKR,,ICMprofile,0,"M2250D.ICM"

[M2250D_Analog.CopyFiles]
M2250D.ICM

[Strings]
DiskName="LG Monitor Profiles Installation Disk"
LG="LG"
M2250D_Analog="LG M2250D"
