[Version]
signature="$CHICAGO$"
Class=Monitor
ClassGuid={4D36E96E-E325-11CE-BFC1-08002BE10318}
Provider=%AOC%
Catalogfile=212Va.cat
DriverVer=03/18/2007,1.0.0.0

[ControlFlags]
ExcludeFromSelect.nt=Monitor\AOC2230

[ClassInstall32]
