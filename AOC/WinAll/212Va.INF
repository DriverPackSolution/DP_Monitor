[Manufacturer]
%AOC%=AOC,ntamd64

; Manufacturer Sections
;-------------------------------------------------
[AOC]
%2230%=2230.Install, Monitor\AOC2230

[AOC.ntamd64]
%2230%=2230.Install, Monitor\AOC2230

; Install Sections
;-------------------------------------------------
[2230.Install]
DelReg=DEL_CURRENT_REG
AddReg=2230.AddReg, 1680, DPMS
Copyfiles=2230.CopyFiles


; Common AddReg/DelReg sections
;-------------------------------------------------
[DEL_CURRENT_REG]
HKR,MODES
HKR,,MaxResolution
HKR,,DPMS
HKR,,ICMProfile


[1680]
HKR,,MaxResolution,,"1680,1050"
[DPMS]
HKR,,DPMS,,1

; Model AddReg sections
;-------------------------------------------------
[2230.AddReg]
HKR,"MODES\1680,1050",Mode1,,"31.0-80.0,56.0-75.0,+,+"
HKR,,ICMProfile,0,"212Va.icm"


[2230.CopyFiles]
212Va.icm


